@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Login SCOI Grupo Izagar.</title>
    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/css/custom")
    @Styles.Render("~/Content/fontAwesome")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/src/styles/login/main.css" rel="stylesheet" />
    <style type="text/css">
        .auto-style1 {
            width: 100%;
        }

        .auto-style2 {
            width: 57px;
        }

        .menu {
            font-size: 20px;
            float: right;
            z-index: 9999;
            margin-top: -70px
        }

        #Button1 {
            font-size: 17px
        }
    </style>
</head>

<body class="login">
    <div class="alert" id="mensaje_alerta" style="left:4px;top:7px;right:4px;display:none;position:absolute;z-index:9999;position:fixed">
        <a class="close fa fa-close"></a>
        <strong>Alerta!</strong> <span style="margin-left:10px" id="texto_alerta">Error!!!</span>
    </div>
    <div>
        <div class="login_wrapper container" style="margin-top:50px">
            <div class="animate form login_form">

                <section style="text-align:center;justify-content:center;align-content:center;align-items:center" class="login_content">
                    <i class="btn btn-default menu"
                       onclick="mostrar_modal(true)">
                        <i class="fa fa-bars"></i>
                    </i>
                    <form style="margin-top:-30px" class="container">
                        <h1 style="padding:10px;margin-top:-10px">INICIO</h1>
                        <div style="text-align: center;display:inline-block;width:220px;height:50px" class="form-inline">
                            <i class="glyphicon glyphicon-user" style="font-size:18px;float:left"></i>
                            <input name="nombre" class="form-control" id="nombre" type="text" style="height:35px;width:180px;" placeholder="USUARIO." />
                        </div>
                        <div style="text-align: center;display:inline-block;width:220px;height:50px" class="form-inline">
                            <i class="glyphicon glyphicon-lock" style="font-size:18px;float:left"></i>
                            <input name="pasword" class="form-control" id="passw" style="height:35px;width:180px;" type="password" placeholder="CONTRASEÑA." />
                        </div>
                        <br />
                        <button style="width:170px;margin-left:20px" id="formulario" class="glyphicon glyphicon-log-in btn btn-primary" type="submit"> ENTRAR</button>
                    </form>
                    <div style="height:115px;margin-top:-10px">
                        <img src="data:image/png;base64,"
                             style="height:90px" />
                    </div>
                    <div><p>SCOI WEB © <label>@ViewBag.anio .</label></p></div>
                </section>
            </div>
        </div>
    </div>
    <div class="modal_menu">
        <div class="panel panel-default">
            <div class="panel-heading">
                <i class="fa fa-close close" onclick="mostrar_modal(false)"></i>
                <i class="glyphicon glyphicon-link"> <strong> Accesos</strong></i>
            </div>
            <div class="panel-body">
                <div>
                    <a href="globales/CodigoBarrasGrande"><i class="fa fa-barcode"> </i>Crear Etiquetas.</a>
                    <hr />
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        function mostrar_modal(estado) {
            console.log(estado);
            document.querySelector(".modal_menu").style.display = estado ? "flex" : "none";
        }
    </script>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")


    <script src="@Url.Content("~/Scripts/generales/alertas.js")"></script>
    <script src="@Url.Content("~/Scripts/Sesion/login.js")?1.0.2"></script>

</body>
</html>
